<h1>New Advertising Platform </h1>

<%= simple_form_for @advertising_platform do |f| %>
  <%= f.error_notification %>
  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :url %>
    <%= f.input :active%>
  </div>
  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>